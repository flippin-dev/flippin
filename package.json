		"vite": "^5.0.3",
		"vitest": "^1.6.0"
	},
	"type": "module",
	"dependencies": {
		"@leveluptuts/svelte-fit": "^1.0.3",
		"dayjs": "^1.11.11",
		"svelte-select": "^5.8.3"
